if __name__ == '__main__':
    x = int(input())
    y = int(input())
    z = int(input())
    n = int(input())
    xlist = [num for num in range(x+1)]
    ylist = [num for num in range(y+1)]
    zlist = [num for num in range(z+1)]
    print([[i,j,k] for i in xlist for j in ylist for k in zlist if sum([i,j,k]) != n])
