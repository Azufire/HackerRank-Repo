#include <iostream>
#include <cstdio>
#include <bits/stdc++.h>
using namespace std;

/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/
int max_of_four(int a, int b, int c, int d){
    int nums[4] = {a, b, c, d};
    int len = sizeof(nums)/sizeof(nums[0]);
    std::sort(nums, nums + len);
    return nums[3];
}

int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans = max_of_four(a, b, c, d);
    printf("%d", ans);
    
    return 0;
}
